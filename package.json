  "scripts": {
    "test": "jest src --silent",
    "test:watch": "jest --watch",
    "lint": "eslint  --rulesdir ./scripts/eslint-rules --ext .ts --ext .tsx src",
    "lint:style": "stylelint src/**/*.scss",
    "typecheck": "tsc",
    "strings-check": "node ./scripts/yaml-strings-check.js",
    "clean": "rm -rf dist && rm -rf node_modules/.cache",
    "dev:cypress": "NODE_ENV=development TARGET_LOCALHOST=false CYPRESS=true BASE_URL=http://localhost:8080 NODE_OPTIONS=\"--max-old-space-size=6144\" webpack serve --progress --config configs/webpack.dev.js --config configs/webpack.monaco.js",
    "dev:cypress-cov": "NODE_ENV=development TARGET_LOCALHOST=false CYPRESS=true CYPRESS_COVERAGE=true BASE_URL=http://localhost:8080 NODE_OPTIONS=\"--max-old-space-size=6144\" webpack serve --progress --config configs/webpack.dev.js",
    "dev": "NODE_ENV=development webpack serve --progress --config configs/webpack.dev.js --config configs/webpack.monaco.js",
    "watch": "yarn clean && NODE_ENV=development webpack --watch --progress --config configs/webpack.dev.js --config configs/webpack.monaco.js",
    "services": "node scripts/services.mjs",
    "preqa-services": "node scripts/pre-qa-services.mjs",
    "services:rq": "node scripts/services-rq.mjs",
    "build": "yarn clean && GIT_COMMIT=$(git log -1 --pretty=%h) GIT_BRANCH=$(git rev-parse --abbrev-ref HEAD) webpack --config configs/webpack.prod.js --config configs/webpack.monaco.js",
    "build:cypress": "yarn clean && NODE_ENV=development CYPRESS=true NODE_OPTIONS=\"--max-old-space-size=6144\" webpack --progress --config configs/webpack.dev.js --config configs/webpack.monaco.js",
    "build:cypress-cov": "yarn clean && NODE_ENV=development CYPRESS=true CYPRESS_COVERAGE=true NODE_OPTIONS=\"--max-old-space-size=6144\" webpack --progress --config configs/webpack.dev.js --config configs/webpack.monaco.js",
    "coverage": "yarn test --coverage",
    "grouped-coverage": "yarn coverage && jest-grouped-coverage -i coverage/coverage-summary.json -c coverage-groups.json",
    "docker": "sh -c 'docker build -t harness/nextgenui:\"$0\" . && docker push harness/nextgenui:\"$0\"'",
    "setup-github-registry": "sh scripts/setup-github-registry.sh",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "generate-graphql-types": "graphql-codegen --config graphql-codegen.yml",
    "strings": "npm-run-all strings:*",
    "strings:check": "node scripts/strings/yamlStringsCheck.mjs",
    "strings:genTypes": "node scripts/strings/generateTypesCli.mjs",
    "strings:loaders": "node scripts/strings/generateStrings.mjs",
    "fmt": "prettier --write \"./src/**/*.{ts,tsx,css,scss}\"",
    "generate-merged-coverage": "node mergedCoverage.js",
    "generate-microfrontend-types": "rm -rf src/microfrontends/dist && webpack --config src/microfrontends/webpack.config.js",
    "generate-certificate": "sh scripts/generate-certificate.sh",
    "generate-eslint": "node scripts/generate-eslint-config-for-imports.mjs",
    "build-docker": "sh scripts/docker-build.sh",
    "lighthouse": "node scripts/lighthouse/lighthouse.js",
    "custom-coverage": "npx new-files-coverage-check --e !**/*.test.tsx !**/*.test.ts !**/*.snap !**/*.js !**/*.scss !**/*.mock.ts !**/*.spec.ts !**/*.d.ts !**/*.json !**/*.types.ts !**/types.ts !**/*.yml !**/*.yaml !**cypress/** !**/cypress/** !**/__test*/** !**/*.png !**/*.ico !**/*.svg !**/src/services/** !**/src/queries/** !**/src/**/__mocks__/** !**/src/**/RouteDestinations.tsx !**/*.sh !**/*.md !**/index.ts !**/src/modules/75-chaos/**"
